<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        android:id="@+id/list_item_kep"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/is_account_tie"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="20dp"
    />
    <TextView
        android:id="@+id/list_item_vezeteknev"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="vezeteknev"
        android:textSize="@dimen/list_item_szovegmeret"
        app:layout_constraintStart_toEndOf="@id/list_item_kep"
        app:layout_constraintTop_toTopOf="@id/list_item_kep"
        android:layout_marginStart="20dp"
    />
    <TextView
        android:id="@+id/list_item_keresztnev"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="keresztnev"
        android:textSize="@dimen/list_item_szovegmeret"
        app:layout_constraintStart_toEndOf="@id/list_item_kep"
        android:layout_marginStart="20dp"
        app:layout_constraintBottom_toBottomOf="@id/list_item_kep"
        />

</androidx.constraintlayout.widget.ConstraintLayout>